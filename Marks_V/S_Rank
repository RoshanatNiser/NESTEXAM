<body>

  <h1>Find Your NEST Rank Through Your Marks</h1>
  <input type="number" id="marksInput" placeholder="Enter your marks" />
  <button onclick="estimateRank()">Check Rank</button>
  <div id="result"></div>

  <script>
    function estimateRank() {
      const marks = parseFloat(document.getElementById("marksInput").value);
      const rankData = [
        [145, Infinity, "1 – 10"],
        [140, 144.99, "11 – 20"],
        [137, 139.99, "21 – 30"],
        [135, 136.99, "31 – 40"],
        [133, 134.99, "41 – 49"],
        [132, 132.99, "50 – 55"],  // <-- 133 goes here now
        [123, 131.99, "56 – 90"],
        [112, 122.99, "91 – 173"],
        [102, 111.99, "174 – 309"],
        [96, 101.99, "310 – 440"],
        [85, 95.99, "441 – 819"],
        [80, 84.99, "820 – 1056"],
        [78, 79.99, "1057 – 1187"],
        [70, 77.99, "1188 – 1300"],
        [95, 96.99, "1301 – 1450"],
        [60, 69.99, "1451 – 1500"],
        [70, 72, "1800 – 1900"],  // specific case
        [0, 59.99, "1501+"]
      ];

      let result = "Invalid marks";
      for (let [min, max, rank] of rankData) {
        if (marks >= min && marks <= max) {
          result = Estimated Rank Range: ${rank};
          break;
        }
      }

      document.getElementById("result").innerText = result;
    }
  </script>

</body>
